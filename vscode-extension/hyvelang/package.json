{
  "name": "hyvelang",
  "displayName": "HyveLang",
  "description": "Provides the required tooling to edit Hyve code from vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "hyve",
      "aliases": ["Hyve", "hyve"],
      "extensions": [".hyve",".hvs"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./images/logo.png",
        "dark": "./images/logo.png"
      }
    }],
    "grammars": [{
      "language": "hyve",
      "scopeName": "source.hyve",
      "path": "./syntaxes/hyve.tmLanguage.json"
    }]
  }
}
